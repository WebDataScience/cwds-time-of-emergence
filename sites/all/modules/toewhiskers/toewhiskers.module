    $prettynames['confidence'] = array(
      '5'	=>	'Slow',
      '50'	=>	'Central',
      '95'	=>	'Fast',
    );
    $prettynames['tolerance'] = array(
      '95'	=>	'Low (to extreme 10% of 1950-1999 conditions))',
      '80'	=>	'High (to extreme 40% of 1950-1999 conditions)',
